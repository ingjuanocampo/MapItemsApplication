<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".MapsActivity"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <fragment
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/list_container"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:id="@+id/map"
            android:name="com.juanocampo.mytaxy.test.view.fragment.HamburgMapFragment"/>

    <fragment
            app:layout_constraintTop_toBottomOf="@id/map"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/list_container"
            android:layout_width="match_parent"
            android:layout_height="@dimen/list_size"
            android:name="com.juanocampo.mytaxy.test.view.fragment.TaxisListFragment"
    />

</android.support.constraint.ConstraintLayout>
